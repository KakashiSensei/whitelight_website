{
    "presets": [
        "react",
        "es2015",
        "stage-2"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-decorators",
        "transform-object-rest-spread"
    ],
    "env": {
        "node": {
            "plugins": [
                [
                    "babel-plugin-transform-require-ignore",
                    {
                        "extensions": [
                            ".less",
                            ".css",
                            ".png"
                        ]
                    }
                ]
            ]
        }
    }
}